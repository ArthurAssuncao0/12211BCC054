int main() {
    int n, n0, n2;
    printf(" Apresente um numero inteiro\n");
    scanf("%d", &n);
    n0 = n - 1;
    n2 = n + 1;
    printf(" O antecessor desse numero eh: %d \n", n0);
    printf("O sucessor desse numero eh: %d \n", n2);

return 0;

}

