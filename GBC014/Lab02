#include <stdio.h>

int main (){
    float temp, c;
    printf ("Apresente um valor em fahrenheit:\n");
    scanf ("%f", &temp);
    c = 5 * (temp - 32)/9;
    printf("Essa temperatura em celsius eh:\n %.2f", c);

return 0;


}

