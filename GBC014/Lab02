#include <stdio.h>

int main(){
        int n, r1, r2, r3;
        printf("Digite um numero inteiro positivo de tres digitos:\n");
        scanf("%d", &n);
        r1 = n%10;
        n = n/10;
        r2 = n%10;
        n = n/10;
        r3 = n%10;
        n = n/10;
        printf("O numero gerado eh igual a:\n %d %d %d", r1, r2, r3);

return 0;

}

47

#include <stdio.h>

int main(){
        int n, d1, d2, d3, d4;
        printf("Digite um numero inteiro de quatro digitos;\n");
        scanf("%d", &n);
        d1 = n / 1000;
        d2 = (n%1000) / 100;
        d3 = ((n%1000) % 100) / 10;
        d4 = (((n%1000) % 100) % 10);
        printf("Valor imprimido:\n %d \n %d \n %d \n %d", d1, d2, d3, d4);
		
return 0;

}

48

#include <stdio.h>

int main(){
        int ns, h, m, s;
        printf("Digite um valor inteiro em segundos;\n");
        scanf("%d", &ns);
        h = ns / 3600;
        m = (ns % 3600) / 60;
        s = ((ns % 3600) % 60);
        printf("Valor imprimido:\n %d %d %d", h, m, s);

return 0;

}

49

#include <stdio.h>

int main(){
        int x1, x2, x3, d, h, m, s, s1, s2, s3;
        printf("Digite a hora, minutos e segundos que a experiencia comecou:\n");
        scanf("%d %d %d", &x1, &x2, &x3);
        printf("Digite a duracao da experiencia:\n");
        scanf("%d", &d);
        h = d / 3600;
        m = (d % 3600) / 60;
        s = ((d % 3600) % 60);
        s1 = h + x1;
        s2 = m + x2;
        s3 = s + x3;
        //tentei interpretar de um modo diferente, talvez possa dar problema

        printf("A hora final do experimento eh:\n %d %d %d", s1, s2, s3);

return 0;


}

50

int main(){
        int an, id, aa;
        printf("Digite a idade de um pessoa e o ano atual que ela viva:\n");
        scanf("%d %d", &id, &aa);
        an = aa - id;
        printf ("Essa pessoa nasceu em:\n %d", an);

return 0;


}

51

#include <stdio.h>
#include <math.h>

int main (){
    float x1, x2, y1, y2, d;
    printf("Digite as coordenadas de x:\n");
    scanf("%f %f", &x1, &x2);
    printf("digite as coordenadas de y:\n");
    scanf("%f %f", &y1, &y2);
    d = sqrt(pow((x2 - x1),2) + pow((y2 - y1),2));
    printf("A distancia da origem eh:\n %.1f", d);



return 0;


}

52

int main (){
    float a1, a2, a3, r1, r2, r3, vt, pr;
    printf("Digite quanto cada amigo investiu:\n");
    scanf("%f %f %f", &a1, &a2, &a3);
    printf("Digite o valor do premio:\n");
    scanf("%f", &pr);
    vt = a1 + a2 +a3;
    r1 = a1 / vt * pr;
    r2 = a2 / vt * pr;
    r3 = a3 / vt * pr;
    printf("Cada amigo receberam, respectivamente, o valor de:\n R$ %.2f \n R$ %.2f \n R$ %.2f", r1, r2, r3);



return 0;


}

53

#include <stdio.h>

int main (){
    float c, l, tp, p, pr;
    printf("Apresente o valor do terreno em comprimento e largura, respectivamente:\n");
    scanf("%f %f", &c, &l);
    printf("Digite o valor do preco do metro:\n");
    scanf("%f", &p);
    pr = c + l;
    tp = pr * p;

    printf("O custo para cercar este terreno eh:\n %.2f", tp);


return 0;


}
