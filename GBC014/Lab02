#include <stdio.h>
#include <math.h>

int main() {
    float a, b, hip;
    printf(" Apresente os catetos de um triangulo:\n");
    scanf("%f %f",&a, &b);
    hip = sqrt((a*a)+(b*b));
    printf(" A hipotenusa eh igual a: %f", hip);
       	
return 0;

}

36

#include <stdio.h>

int main() {
    float Alt, vol, R, pi;
    pi = 3.141592;
    printf(" Apresente a altura e o raio de um cilindro:\n");
    scanf("%f %f", &Alt, &R);
    vol = pi * (R * R) * Alt;
    printf(" O volume do cilindro eh igual a: %.6f", vol);
        
return 0;

}

