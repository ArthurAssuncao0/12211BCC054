42

#include <stdio.h>

int main(){
        float SB, bonus, imposto;
        printf("Escreva o salario de um funcionario:\n");
        scanf ("%f", &SB);
        imposto = SB * 0.07;
        bonus = SB * 1.05;
        SB = bonus - imposto;
        printf("O salario a receber eh:\n %.2f", SB);

return 0;

}

43

#include <stdio.h>

int main(){
        float Vt, Vd, P, Vp, Cd, Cp;
        P = 3; //numero de parcelas
        printf("Escreva um valor total:\n");
        scanf("%f", &Vt);
        Vd = Vt * 0.9;
        Vp = Vt / P;
        Cd = Vd * 0.05;
        Cp = Vt * 0.05;
        printf("OS valores sao:\n %.2f %.2f %.2f %.2f", Vd, Vp, Cd, Cp);

return 0;

}

